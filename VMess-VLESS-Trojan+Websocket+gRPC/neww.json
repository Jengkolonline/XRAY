              "certificateFile": "/etc/xray/xray.crt",
              "keyFile": "/etc/xray/xray.key"              
            }
          ],
          "alpn": [
            "h2",
            "http/1.1"
          ]
        }
      },
      "domain": "$domain",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ]
}
END

